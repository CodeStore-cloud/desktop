<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Tooltip?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.HBox?>
<AnchorPane xmlns="http://javafx.com/javafx" xmlns:fx="http://javafx.com/fxml"
            fx:controller="cloud.codestore.client.ui.history.History"
            stylesheets="@history.css">
    <HBox>
        <Button fx:id="prevSnippetButton" focusTraversable="false" onAction="#selectPreviousSnippet">
            <tooltip>
                <Tooltip text="%history.prev"/>
            </tooltip>
        </Button>
        <Button fx:id="nextSnippetButton" focusTraversable="false" onAction="#selectNextSnippet">
            <tooltip>
                <Tooltip text="%history.next"/>
            </tooltip>
        </Button>
    </HBox>
</AnchorPane>
